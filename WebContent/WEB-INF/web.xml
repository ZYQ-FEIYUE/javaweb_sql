<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>AndroidTest</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.servlet.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Test</servlet-name>
		<servlet-class>com.servlet.Test</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Test</servlet-name>
		<url-pattern>/mustTest</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Register</servlet-name>
		<servlet-class>com.servlet.Register</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/Register</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeviceMessage</servlet-name>
		<servlet-class>com.servlet.DeviceMessage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeviceMessage</servlet-name>
		<url-pattern>/DeviceMessage</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeviceQuery</servlet-name>
		<servlet-class>com.servlet.DeviceQuery</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeviceQuery</servlet-name>
		<url-pattern>/DeviceQuery</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeviceShare</servlet-name>
		<servlet-class>com.servlet.DeviceShare</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeviceShare</servlet-name>
		<url-pattern>/DeviceShare</url-pattern>
	</servlet-mapping>
</web-app>